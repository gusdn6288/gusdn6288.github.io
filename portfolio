	<script>
		$(window).load(function() {
			design_mode_loader.end()
		});

		function designModePreLoader(){
			var $loader  = $(".se-pre-con");
			var $steps = $('#dsPreload').find('._step');
			var index = 0;

			var start = function(){
				$steps.removeClass('loading');
				$steps.eq(index).addClass('loading');

				index++;
			};

			var end = function(){
				$loader.fadeOut("slow");
			};

			return {
				'start' : function(){
					start();
				},
				'end' : function(){
					end();
				}
			}
		}
		var design_mode_loader = new designModePreLoader();

		var container_width = 980;
		var design;
		$(function() {
			/* Bootstrap Sanitizer Custom */
			var customTooltipAllowList = $.fn.tooltip.Constructor.DEFAULTS.whiteList;
			customTooltipAllowList.table = [];
			customTooltipAllowList.thead = [];
			customTooltipAllowList.tbody = [];
			customTooltipAllowList.tr = [];
			customTooltipAllowList.td = ["rowspan", "colspan"];
			customTooltipAllowList.th = [];
			customTooltipAllowList.caption = [];
			customTooltipAllowList["*"].push("style");
			/* End Bootstrap Sanitizer Custom */

			MENU.init('m2022053188fdf4d782762', design);
						SHOP_PROD_MANAGE.addUnitData('u202205316295d520de69d','KR','₩', ',', '.', 0);
						// TODO 페이스북 캐시 삭제 페이지 https://developers.facebook.com/tools/debug/og/object/
			// TODO 카카오스토리 캐시 삭제 페이지 https://developers.kakao.com/docs/restapi/tool
			// TODO 캐싱 url 형식 : http://cocoaphp.com/test/kakaoStory.php?u={request_url}
		});
	</script>

